# my first code
if __name__ == '__main__':
    n = int(input())
    arr = map(int, input().split())
    arr = list(arr)
    
    arr.sort(reverse = True)
    
    for i in arr:
        if i < max(arr):
            print(i)
            break
            
#recommended code
i = int(input())
lis = list(map(int,raw_input().strip().split()))[:i]
z = max(lis)
while max(lis) == z:
    lis.remove(max(lis))

print max(lis)
